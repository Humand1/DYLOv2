{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "backend/main.py": {
      "runtime": "@vercel/python",
      "maxDuration": 60
    }
  }
}
